{
  "name": "@shinspiegel/use-bem-css",
  "version": "0.2.0",
  "description": "A hook to generate BEM styled string to use on classNames for react components",
  "homepage": "https://shinspiegel.github.io/use-bem-css/",
  "author": "ShinSpiegel",
  "license": "MIT",
  "repository": "shinspiegel/use-bem-css",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "typescript": ">=4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/runtime": "^7.18.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@rollup/plugin-url": "^7.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.5.1",
    "@types/react": "^17.0.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^4.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "^5.0.1",
    "rollup": "^2.74.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.4.0",
    "typescript": "^4.6"
  }
}
